#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib64/ivl/system.vpi";
:vpi_module "/usr/lib64/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib64/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib64/ivl/v2005_math.vpi";
:vpi_module "/usr/lib64/ivl/va_math.vpi";
:vpi_module "/usr/lib64/ivl/v2009.vpi";
S_0x55fe8ac57530 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x55fe8ac576c0 .scope module, "standard" "standard" 3 1;
 .timescale 0 0;
v0x55fe8aca3c00_0 .var "clk", 0 0;
v0x55fe8aca3ca0_0 .net "count", 3 0, v0x55fe8ac7e8e0_0;  1 drivers
v0x55fe8aca3d40_0 .var "rst", 0 0;
S_0x55fe8ac91df0 .scope module, "countunit" "counter" 3 7, 4 1 0, S_0x55fe8ac576c0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /OUTPUT 4 "count";
v0x55fe8ac7e4e0_0 .net "clk", 0 0, v0x55fe8aca3c00_0;  1 drivers
v0x55fe8ac7e8e0_0 .var "count", 3 0;
v0x55fe8aca3ae0_0 .net "rst", 0 0, v0x55fe8aca3d40_0;  1 drivers
E_0x55fe8ac92f50 .event posedge, v0x55fe8ac7e4e0_0;
S_0x55fe8ac91c60 .scope module, "tb" "tb" 5 4;
 .timescale 0 0;
    .scope S_0x55fe8ac91df0;
T_0 ;
    %wait E_0x55fe8ac92f50;
    %load/vec4 v0x55fe8aca3ae0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x55fe8ac7e8e0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55fe8ac7e8e0_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x55fe8ac7e8e0_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55fe8ac576c0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55fe8aca3c00_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55fe8aca3d40_0, 0, 1;
    %end;
    .thread T_1, $init;
    .scope S_0x55fe8ac576c0;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x55fe8aca3c00_0;
    %inv;
    %store/vec4 v0x55fe8aca3c00_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x55fe8ac576c0;
T_3 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55fe8aca3d40_0, 0, 1;
    %pushi/vec4 2, 0, 32;
T_3.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.1, 5;
    %jmp/1 T_3.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x55fe8ac92f50;
    %jmp T_3.0;
T_3.1 ;
    %pop/vec4 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55fe8aca3d40_0, 0, 1;
    %pushi/vec4 10, 0, 32;
T_3.2 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.3, 5;
    %jmp/1 T_3.3, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x55fe8ac92f50;
    %jmp T_3.2;
T_3.3 ;
    %pop/vec4 1;
    %vpi_call/w 3 24 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x55fe8ac91c60;
T_4 ;
    %vpi_call/w 5 7 "$dumpfile", "Counter/wave.vcd" {0 0 0};
    %vpi_call/w 5 8 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55fe8ac576c0 {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 6;
    "N/A";
    "<interactive>";
    "-";
    "Counter/../Tb_Sanitation/tb_standard.vh";
    "Counter/counter.v";
    "Counter/tb_counter.v";
