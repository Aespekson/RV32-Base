#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib64/ivl/system.vpi";
:vpi_module "/usr/lib64/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib64/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib64/ivl/v2005_math.vpi";
:vpi_module "/usr/lib64/ivl/va_math.vpi";
:vpi_module "/usr/lib64/ivl/v2009.vpi";
S_0x55672a52d560 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x55672a52d6f0 .scope module, "tb" "tb" 3 1;
 .timescale 0 0;
v0x55672a579690_0 .var "clk", 0 0;
v0x55672a579730_0 .net "count", 3 0, v0x55672a554540_0;  1 drivers
v0x55672a5797d0_0 .var "rst", 0 0;
S_0x55672a5683b0 .scope module, "uut" "counter" 3 10, 4 1 0, S_0x55672a52d6f0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /OUTPUT 4 "count";
v0x55672a554140_0 .net "clk", 0 0, v0x55672a579690_0;  1 drivers
v0x55672a554540_0 .var "count", 3 0;
v0x55672a579570_0 .net "rst", 0 0, v0x55672a5797d0_0;  1 drivers
E_0x55672a568a60 .event posedge, v0x55672a554140_0;
    .scope S_0x55672a5683b0;
T_0 ;
    %wait E_0x55672a568a60;
    %load/vec4 v0x55672a579570_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x55672a554540_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55672a554540_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x55672a554540_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55672a52d6f0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55672a579690_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55672a5797d0_0, 0, 1;
    %end;
    .thread T_1, $init;
    .scope S_0x55672a52d6f0;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x55672a579690_0;
    %inv;
    %store/vec4 v0x55672a579690_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x55672a52d6f0;
T_3 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55672a5797d0_0, 0, 1;
    %pushi/vec4 2, 0, 32;
T_3.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.1, 5;
    %jmp/1 T_3.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x55672a568a60;
    %jmp T_3.0;
T_3.1 ;
    %pop/vec4 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55672a5797d0_0, 0, 1;
    %pushi/vec4 20, 0, 32;
T_3.2 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.3, 5;
    %jmp/1 T_3.3, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x55672a568a60;
    %jmp T_3.2;
T_3.3 ;
    %pop/vec4 1;
    %vpi_call/w 5 13 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x55672a52d6f0;
T_4 ;
    %vpi_call/w 3 17 "$dumpfile", "Counter/wave.vcd" {0 0 0};
    %vpi_call/w 3 18 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55672a52d6f0 {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 6;
    "N/A";
    "<interactive>";
    "-";
    "Counter/tb_counter.v";
    "Counter/counter.v";
    "Counter/../tb_sanitation/tb_standard.vh";
